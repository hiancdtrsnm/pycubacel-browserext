{
    "manifest_version": 2,
    "name": "Micubacel Plugin",
    "description": "Consult micubacel information .This extension runs Python code.",
    "version": "1.1.3",
    "icons": {
        "16": "icons/etecsa.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "micubacel@plugin.cu",
            "strict_min_version": "57.0"
        }
    },
    "browser_action": {
        "default_icon": "icons/etecsa.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "alarms",
        "activeTab",
        "https://ajax.googleapis.com/",
        "*://127.0.0.1/*"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
