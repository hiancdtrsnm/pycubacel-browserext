{
    "manifest_version": 2,
    "name": "Python Chrome Plugin",
    "description": "This extension runs Python code.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "etecsa.ico",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "*://127.0.0.1/*"
    ]
}
